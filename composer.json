{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/addtoany": "^1.12",
        "drupal/addtocal": "1.x-dev",
        "drupal/admin_toolbar": "^1.27",
        "drupal/adminimal_admin_toolbar": "^1.9",
        "drupal/adminimal_theme": "^1.5",
        "drupal/advagg": "^4.0",
        "drupal/amp": "^2.1",
        "drupal/amptheme": "^1.1",
        "drupal/autologout": "^1.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/blazy": "^2.0@RC",
        "drupal/block_class": "^1.0",
        "drupal/calendar": "1.x-dev",
        "drupal/captcha": "^1.0@beta",
        "drupal/chosen": "^2.8",
        "drupal/cloudflare": "^1.0@alpha",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_partial_export": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/content_lock": "^1.0@alpha",
        "drupal/core": "^8.7.0",
        "drupal/date_recur": "^2.0",
        "drupal/dynamic_entity_reference": "^1.7",
        "drupal/encrypt": "^3.0@RC",
        "drupal/entity_browser": "^2.2",
        "drupal/entity_browser_enhanced": "^1.0",
        "drupal/entity_clone": "^1.0@beta",
        "drupal/entity_reference_layout": "^1.0@alpha",
        "drupal/entityqueue": "^1.0@beta",
        "drupal/facets": "^1.4",
        "drupal/field_group": "^3.0",
        "drupal/flexible_layout": "^1.0@beta",
        "drupal/flickity": "^2.2",
        "drupal/focal_point": "^1.2",
        "drupal/fontawesome": "^2.14",
        "drupal/fontyourface": "^3.2",
        "drupal/fullcalendar_view": "^2.3",
        "drupal/ga_login": "^1.0@alpha",
        "drupal/geocoder": "^2.3",
        "drupal/geolocation": "^1.11",
        "drupal/google_analytics": "^3.0",
        "drupal/google_analytics_reports": "^3.0@beta",
        "drupal/google_map_field": "^1.5",
        "drupal/gutenberg": "^1.6",
        "drupal/image_widget_crop": "^2.2",
        "drupal/imageapi_optimize": "^2.0",
        "drupal/imageapi_optimize_binaries": "^1.0@alpha",
        "drupal/imageapi_optimize_tinypng": "^1.0@beta",
        "drupal/images_optimizer": "^1.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/key": "^1.10",
        "drupal/layout_builder_restrictions": "^2.2",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/layout_library": "^1.0@beta",
        "drupal/length_indicator": "^1.0@RC",
        "drupal/libraries": "^3.0@alpha",
        "drupal/link_attributes": "^1.9",
        "drupal/login_security": "^1.5",
        "drupal/moderation_dashboard": "^1.0@beta",
        "drupal/name": "^1.0@RC",
        "drupal/obfuscate_email": "^1.1",
        "drupal/paragraphs": "^1.9",
        "drupal/password_policy": "^3.0@alpha",
        "drupal/pathauto": "^1.4",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/real_aes": "^2.2",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.4",
        "drupal/rename_admin_paths": "^1.1",
        "drupal/responsive_preview": "^1.0@beta",
        "drupal/sa11y": "^1.0@beta",
        "drupal/schema_metatag": "^1.4",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_sorts": "^1.0@beta",
        "drupal/select2": "^1.4",
        "drupal/shariff": "^1.5",
        "drupal/simple_sitemap": "^3.3",
        "drupal/superfish": "^1.3",
        "drupal/tfa": "^1.0@alpha",
        "drupal/tvi": "^1.0@beta",
        "drupal/twig_field_value": "^1.2",
        "drupal/username_enumeration_prevention": "^1.0@beta",
        "drupal/video_embed_field": "^2.2",
        "drupal/views_block_filter_block": "^1.0@beta",
        "drupal/views_infinite_scroll": "^1.6",
        "drupal/webp": "^1.0",
        "drupal/yoast_seo": "^1.5",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
